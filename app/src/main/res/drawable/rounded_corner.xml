<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- view background color a9c5ac-->
    <!--http://tips.androidhive.info/2013/09/android-layout-rounded-corner-border/-->
    <solid
        android:color="#ffffff" >
    </solid>

    <!-- view border color and width   -->
    <stroke
        android:width="5dp"
        android:color="#B2EBF2" >
    </stroke>

    <!-- If you want to add some padding F44336 -->
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"    >
    </padding>

    <!-- Here is the corner radius -->
    <corners
        android:radius="10dp"   >
    </corners>

</shape>